# --- Private / User-local (NEVER commit) ---
.claude/cwe-settings.yml
.claude/agent-memory/
.claude/agent-memory-local/
docs/plans/
*.local.md
*.local.json
*.local.yml

# --- Credentials & Secrets ---
.env*
*.pem
*.key
*.pfx
*.p12
*.crt
id_rsa*
id_ed25519*
id_ecdsa*

# --- IDE / Editor ---
.vscode/settings.json
.vscode/*.code-workspace
.idea/

# --- OS ---
.DS_Store
Thumbs.db
Desktop.ini

# --- Dependencies ---
node_modules/
__pycache__/
.venv/
vendor/

# --- Build / Cache ---
dist/
build/
*.pyc
.cache/

# --- Tool caches ---
.serena/
_backup/
